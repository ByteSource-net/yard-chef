<% define_list = item_listing(:definition) %>
<% if define_list[0].size > 0 %>
  <h2>Definition Summary
     <small>(<a href="#" class="summary_toggle">collapse</a>)</small>
  </h2>
  <ul class="summary">
    <% define_list[0].each do |define| %>
      <%= yieldall :item => define %>
    <% end %>
  </ul>
<% end %>
