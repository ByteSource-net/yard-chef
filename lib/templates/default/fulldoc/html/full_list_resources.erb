<% n = 1 %>
<% @items.each do |item| %>
  <li class="r<%= n %>">
    <span class="object_link">
      <a href="Chef/Resource.html#<%= item.parent.name %>" title="<%= item.parent.name %>"><%= h(item.name) %></a>
      </span>
      <small><%= item.namespace %></small>
  </li>
  <% n = n == 2 ? 1 : 2 %>
<% end %>
