<% n = 1 %>
<% @items.each do |item| %>
  <li class="r<%= n %>">
    <span class="object_link">
      <a href="<%= item.parent.parent.name %>/<%= item.parent.name %>.html#<%= item.name %>" 
        title="<%= item.path %> (Resource)">
        <%= h(item.name) %>
      </a>
      </span>
      <small><%= item.namespace %></small>
  </li>
  <% n = n == 2 ? 1 : 2 %>
<% end %>
