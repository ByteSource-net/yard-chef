<% n = 1 %>
<% @items.each do |item| %>
  <li class="r<%= n %>">
    <span class="object_link">
      <a href="<%= url_for(item.parent, item.name, false) %>" 
         title="<%= item.file %>">
        <%= h(item.name) %>
      </a>
    </span>
    <small><%= item.namespace.parent.path %></small>
  </li>
  <% n = n == 2 ? 1 : 2 %>
<% end %>
