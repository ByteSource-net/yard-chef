<% if object.providers.size > 0 %>
  <% providers = object.providers.uniq {|prov| prov.name.to_s} %>
  <li><h4>Providers</h4>
  <ul class="summary compact">
    <% providers.each do |provider| %>
      <% cookbook = get_cookbook(provider.file) %>
      <li>
        <span class="summary_signature">
          <a href="<%= cookbook.namespace %>/<%= url_for(cookbook, provider.long_name) %>">
            <%= provider.long_name %>
          </a>
        </span>
      </li>
    <% end %>
  </ul></li>
<% end %>
